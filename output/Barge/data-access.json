    "pattern": "Dapper with embedded SQL resources",
    "sqlResourcePattern": "SqlText.GetSqlText(\"FileName\")",
    "examples": [
      {
        "method": "GetByIdAsync",
        "implementation": "var sql = SqlText.GetSqlText(\"Barge_GetById\");\nreturn await _connection.QuerySingleOrDefaultAsync<BargeDto>(sql, new { BargeID = id });"
      },
      {
        "method": "CreateAsync",
        "implementation": "var sql = SqlText.GetSqlText(\"Barge_Insert\");\nvar newId = await _connection.ExecuteScalarAsync<int>(sql, barge);\nbarge.BargeID = newId;\nreturn barge;"
      },
      {
        "method": "SetActiveAsync",
        "implementation": "var sql = SqlText.GetSqlText(\"Barge_SetActive\");\nvar rowsAffected = await _connection.ExecuteAsync(sql, new { BargeID = id, IsActive = isActive });\nreturn rowsAffected > 0;"
      }
    ]
