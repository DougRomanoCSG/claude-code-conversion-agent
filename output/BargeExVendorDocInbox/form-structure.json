    {
      "feature": "Multi-selection with checkbox column",
      "description": "Grid has clickable IsSelected column for multi-row selection",
      "implementation": "Custom click handler inverts checkbox state on cell click"
    },
    {
      "feature": "Fleet-aware search",
      "description": "Search results filtered by currently selected fleet",
      "implementation": "OnChangeOfFleet clears results when fleet changes"
    },
    {
      "feature": "Import validation",
      "description": "Pre-import validation for tickets and revisions",
      "implementation": "NoBargeTicketBargeNums and HasRevisionWithNoPriorImport methods"
    },
    {
      "feature": "Add missing entities",
      "description": "Can add barges, boats, and locations directly from BargeEx data",
      "implementation": "ItemAdd* methods launch detail forms with pre-populated BargeEx values"
    },
    {
