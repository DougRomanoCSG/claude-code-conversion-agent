          "source": "frmBargeSeriesSearch:37-43",
          "implementation": "Creates frmBargeSeriesDetail with BargeSeries.NewBargeSeries()"
        },
        {
          "step": 2,
          "action": "OpenDetailForm",
          "navigation": "Opens frmBargeSeriesDetail as modal dialog",
          "mode": "New",
          "dataPassed": "New BargeSeries business object",
          "formTitle": "New - Barge Series Detail"
        },
        {
          "step": 3,
          "action": "EnterBasicInfo",
          "section": "pnlDetailEdit",
          "fields": [
            {
              "control": "txtName",
              "label": "Series",
              "required": true,
              "validation": "String type, max length from BargeSeries.MaxLength.Name"
            },
            {
              "control": "cboCustomerID",
              "label": "Owner",
              "type": "Dropdown",
              "source": "Lists.CustomerList.GetList"
            },
            {
              "control": "cboHullType",
              "label": "Hull type",
              "type": "ValidationList",
              "source": "ValidationList.ValidationTypes.HullType"
            },
            {
              "control": "cboCoverType",
              "label": "Cover type",
              "type": "ValidationList",
              "source": "ValidationList.ValidationTypes.CoverType"
            }
          ]
        },
        {
          "step": 4,
          "action": "EnterDimensions",
          "section": "pnlDetailEdit",
          "fields": [
            {
              "control": "txtLength",
              "label": "Length",
              "validation": "Decimal"
            },
            {
              "control": "txtWidth",
              "label": "Width",
              "validation": "Decimal"
            },
            {
              "control": "txtDepth",
              "label": "Depth",
              "validation": "Decimal"
            },
            {
              "control": "txtTonsPerInch",
              "label": "Tons/inch",
              "validation": "Decimal"
            },
            {
              "control": "txtDraftLightFeet",
              "label": "Light draft (feet)",
              "validation": "Int32"
            },
            {
              "control": "txtDraftLightInches",
              "label": "Light draft (inches)",
              "validation": "Int32"
            }
          ]
        },
        {
          "step": 5,
          "action": "EnterDraftTonnageData",
          "section": "grdBargeSeriesDraft",
          "description": "User enters tonnage data for drafts 0-13 feet at inch increments (0-11 inches)",
          "gridStructure": {
            "rows": 14,
            "draftRange": "0-13 feet",
            "columns": [
              "DraftFeet (read-only)",
              "Tons00 (0 inches)",
              "Tons01 (1 inch)",
              "Tons02 (2 inches)",
              "Tons03 (3 inches)",
              "Tons04 (4 inches)",
              "Tons05 (5 inches)",
              "Tons06 (6 inches)",
              "Tons07 (7 inches)",
              "Tons08 (8 inches)",
              "Tons09 (9 inches)",
              "Tons10 (10 inches)",
              "Tons11 (11 inches)"
            ]
          },
          "editBehavior": {
            "cellClickAction": "EditAndSelectText",
            "tabNavigation": "NextControlOnLastCell",
            "keyboardNavigation": "Arrow keys move between cells in edit mode",
            "nullDisplay": "xxx",
            "invalidValueBehavior": "RetainValueAndFocus",
            "validation": "Integer values only, non-numeric entries converted to 'xxx'"
          },
          "pasteFromClipboard": {
            "button": "Paste from Clipboard (toolbar)",
            "supportedFormats": ["CSV", "Tab-delimited"],
            "behavior": "Pastes data starting from Tons00 column, validates and converts to integers",
            "source": "frmBargeSeriesDetail:789-850"
          }
        },
        {
          "step": 6,
          "action": "SetActiveStatus",
          "control": "chkIsActive",
          "label": "Active",
          "defaultValue": true
