  "primaryWorkflows": [
    {
      "name": "SearchAndView",
      "description": "User searches for barge series using filter criteria and views results",
      "steps": [
        {
          "step": 1,
          "action": "EnterSearchCriteria",
          "controls": [
            "txtBargeSeries",
            "cboCustomerID",
            "cboHullType",
            "cboCoverType",
            "chkActiveOnly"
          ],
          "details": "User can filter by series name, customer/owner, hull type, cover type, and active status"
        },
        {
          "step": 2,
          "action": "ClickSearch",
          "button": "btnFind",
          "trigger": "btnFind_Click",
          "result": "Calls BargeSeriesSearch.GetList() and displays results in grdSearch grid"
        },
        {
          "step": 3,
          "action": "ReviewResults",
          "control": "grdSearch",
          "displayedColumns": [
            "Name",
            "Customer",
            "HullType",
            "CoverType",
            "Dimensions",
            "DraftLight",
            "TonsPerInch",
            "IsActive"
          ]
        }
      ]
    },
    {
      "name": "CreateNewBargeSeries",
